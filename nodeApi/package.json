{
  "name": "nodeApi",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prod:start": "node ./dist/bundle",
    "dev:start": "NODE_ENV=dev npm-run-all build:start --parallel dev:wabpack dev:nodemon",
    "build:start": "npm-run-all build:clean build:building",
    "dev:wabpack": "webpack --watch",
    "dev:nodemon": "nodemon ./dist/bundle.js",
    "build:clean": "rimraf dist",
    "build:building": "webpack"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.x.x",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "rimraf": "^2.6.2",
    "webpack-node-externals": "^1.6.0",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {
    "react": "15.x.x",
    "react-dom": "15.6.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "fs": "^0.0.1-security",
    "moment": "^2.20.1",
    "mongodb": "^2.2.33",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.4.2",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "underscore": "^1.8.3",
    "webpack": "^3.8.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ]
    }
  },
  "babel": {
    "plugins": [
      "babel-plugin-transform-object-assign"
    ],
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  }
}
